

# Datos proporcionados
notes = [1, 6, 8, 9, 10, 6, 5]
alumnes = ["Jaume", "Carles", "Cristina", "Josep", "Rafael", "Agnès", "Marta"]
cognoms = ["Tort", "Soldevila", "Luna", "Muñoz", "Fernandez", "Hernandez", "Llopart"]


notas_arregladas = []
noms_complets = []
notes_textuals = []
for nota in notes:
    nota = nota + 1
    if nota < 10:
        nota = nota + 1
        notas_arregladas.append(nota)
    else:
        nota
        notas_arregladas.append(nota)

dades= {}

for nom, cognom in zip(alumnes, cognoms):
    nom_complet = nom+" "+cognom
    noms_complets.append(nom_complet)


#quan tens 2 llistes, problema per a resoldre-ho

for n in notas_arregladas:
    if n < 5:
        valor = "suspendido"
        notes_textuals.append(valor)
    elif n >=5 and n <=6:
        valor = "aprobado"
        notes_textuals.append(valor)
    elif n >6 and n <7:
        valor = "bien"
        notes_textuals.append(valor)
    elif n >8 and n <7:
        valor = "notable"
        notes_textuals.append(valor)




dades = {}
llista_final = []

for nota, nom, q in zip(notas_arregladas, noms_complets, notes_textuals):
    dades("nom alumne") = nom
    dades("nota") = nota
    dades("qualificació") = q
    llista_final.append(dades)

df = pd.DataFrame(llista_final)
print(df)
df.to_csv("dades.csv")
